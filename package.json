		"chalk": "^2.1.0",
		"chromedriver": "^2.27.2",
		"copy-webpack-plugin": "^4.0.1",
		"cross-env": "^5.0.5",
		"cross-spawn": "^5.0.1",
		"css-loader": "^0.28.4",
		"del": "^3.0.0",
		"devtron": "^1.4.0",
		"electron": "^1.7.5",
		"electron-builder": "^19.56.2",
		"electron-debug": "^1.4.0",
		"electron-devtools-installer": "^2.2.0",
		"eslint": "^4.4.1",
		"eslint-config-airbnb-base": "^11.2.0",
		"eslint-friendly-formatter": "^3.0.0",
