{
  "name": "jse-desktop",
  "version": "0.6.1",
  "private": true,
  "description": "JSE Platfrom Agnostic APP - Web/Desktop/Mobile/PWA",
  "author": {
    "name": "JSEcoin Ltd",
    "email": "support@jsecoin.com"
  },
  "scripts": {
    "lint": "vue-cli-service lint",
    "all:build": "npm run docs:build && npm run desktop:build && npm run web:build && npm run mobile:android:build:github",
    "desktop:build": "vue-cli-service electron:build",
    "desktop:dev": "vue-cli-service electron:serve",
    "docs:build": "vue-cli-service styleguidist:build",
    "docs:dev": "vue-cli-service styleguidist",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "mobile:android:build": "vue-cli-service cordova-build-android",
    "mobile:android:build:github": "npm run mobile:build && cd mobile && cordova build --release --buildConfig=../../signAndroidBuild.json",
    "mobile:android:build:google": "cross-env ISGOOGLE=TRUE npm run mobile:build && cordova build --release --buildConfig=../../signAndroidBuild.json",
    "mobile:android:dev": "vue-cli-service cordova-serve-android",
    "mobile:browser:build": "vue-cli-service cordova-build-browser",
    "mobile:browser:dev": "vue-cli-service cordova-serve-browser",
    "mobile:build": "vue-cli-service build --dest mobile/www ",
    "mobile:ios:build": "vue-cli-service cordova-build-ios",
    "mobile:ios:dev": "vue-cli-service cordova-serve-ios",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "web:build": "vue-cli-service build",
    "web:dev": "vue-cli-service serve --https --open --mode development --host localhost"
  },
  "main": "background.js",
  "dependencies": {
    "auto-launch": "^5.0.5",
    "axios": "^0.19.0",
    "c3": "^0.6.0",
    "electron-updater": "^4.0.6",
    "intro.js": "^2.9.3",
    "moment": "^2.24.0",
    "pdfmake": "^0.1.57",
    "qrious": "^4.0.2",
    "register-service-worker": "^1.5.2",
    "vue": "^2.6.10",
    "vue-clipboard2": "^0.2.1",
    "vue-electron": "^1.0.6",
    "vue-head": "^2.1.0",
    "vue-introjs": "^1.3.2",
    "vue-json-tree-view": "^2.1.4",
    "vue-moment": "^4.0.0",
    "vue-multianalytics": "^1.10.0",
    "vue-qriously": "^1.1.1",
    "vue-router": "^3.0.1",
    "vue-slider-component": "^2.8.2",
    "vue-social-sharing": "^2.3.3",
    "vue-swal": "^1.0.0",
    "vuebar": "0.0.20",
    "vuex": "^3.0.1",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-plugin-pwa": "^3.2.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "electron": "^6.0.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "prerender-spa-plugin": "^3.4.0",
    "rollup": "^0.67.4",
    "vue-cli-plugin-cordova": "^2.0.8",
    "vue-cli-plugin-electron-builder": "^1.4.0",
    "vue-cli-plugin-styleguidist": "^0.1.9",
    "vue-styleguidist": "^2.1.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.27.1"
  },
  "bugs": {
    "url": "https://github.com/jsecoin/platform/issues"
  },
  "homepage": "https://github.com/jsecoin/platform#readme",
  "keywords": [
    "JSEcoin",
    "Cryptominer",
    "altcoin",
    "platform",
    "adtech",
    "cryptocurrency"
  ],
  "license": "(ISC OR GPL-3.0)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsecoin/platform.git"
  }
